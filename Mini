javascript:var cog_avg=parseFloat(prompt("Cog Skill Average")); var grade_level=parseFloat(prompt("Grade Level")); var power_on_track=parseFloat(prompt("Put 1 if Done on Track for Power Focus Areas; Put 0 if Behind")); var additional_percentage=parseFloat(prompt("Additional Percentage")); alert((15 * (cog_avg - (((grade_level || 9) - 6) * 0.5 + 2)) + 70)* 0.7 + (power_on_track ? 21 : 0.0) + additional_percentage * 0.09 +"%");
